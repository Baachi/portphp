        "ext-mbstring": "*",
        "ext-sqlite3": "*",
        "phpunit/phpunit": "^4.0",
        "phpspec/phpspec": "^2.1",
        "doctrine/dbal": "~2.4",
        "doctrine/orm": "~2.4",
        "phpoffice/phpexcel": "~1.8",
        "symfony/property-access": "2.3",
        "symfony/validator": "~2.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Port\\Tests\\": "tests/"
        }
    }
